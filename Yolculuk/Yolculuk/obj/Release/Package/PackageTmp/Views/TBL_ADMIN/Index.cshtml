@model IEnumerable<Yolculuk.Models.TBL_ADMIN>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Template/Admin_Template.cshtml";
}

<h2 style="text-align:center;color:red">ADMİN</h2>

<p>
    @Html.ActionLink("YENİ ADMİN", "EKLE")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.kullaniciadi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sifre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.kullaniciadi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sifre)
        </td>
        <td>
            @Html.ActionLink("Düzenle", "Duzenle", new { id=item.id }) |
            @Html.ActionLink("Sil", "Sil", new { id=item.id })
        </td>
    </tr>
}

</table>

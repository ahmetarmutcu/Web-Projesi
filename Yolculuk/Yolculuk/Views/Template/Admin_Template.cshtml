@if(Session["kullaniciadi"]==null)
{
    Response.Redirect("~/Ayseren/HataSayfasi");
}
<html lang="tr">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Admin Paneli">
    <meta name="author" content="Ahmet Armutcu">

    <title>ADMİN PANELİ</title>
    <link href="~/Content/Admin/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Admin/css/dataTables.bootstrap4.css" rel="stylesheet" />
    <link href="~/Content/Admin/fontawesome-free/css/all.min.css" rel="stylesheet" />
    <link href="~/Content/Admin/css/sb-admin.css" rel="stylesheet" />
</head>
<body id="page-top">
    <nav class="navbar navbar-expand navbar-dark bg-dark static-top">
        <a class="navbar-brand mr-1" href="index.html">ADMİN PANELİ</a>
        <button class="btn btn-link btn-sm text-white order-1 order-sm-0" id="sidebarToggle" href="#">
            <i class="fas fa-bars"></i>
        </button>
        <ul class="navbar-nav ml-auto ml-md-0">
            <li class="nav-item dropdown no-arrow">
                <a class="nav-link dropdown-toggle" href="#" id="userDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <i class="fas fa-user-circle fa-fw"></i>
                </a>
                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="userDropdown">
                    <a class="dropdown-item" href="~/TBL_ADMIN/Index">Ayarlar</a>
                    <div class="dropdown-divider"></div>
                    <a class="dropdown-item" href="@Url.Action("Cikis","Ayseren",null, null)">Çıkış</a>
                </div>
            </li>
        </ul>
    </nav>
    <div id="wrapper">
        <ul class="sidebar navbar-nav">
            <li class="nav-item">
                <a class="nav-link" href="~/TBL_SLİDER/SliderGosterme">
                    <i class="fas fa-fw fa-chart-area"></i>
                    <span>SLİDER</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="~/TBL_HIZMETLER/Hizmetler">
                    <i class="fas fa-image"></i>
                    <span>Hizmetlerimiz</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="~/TBL_ISTANBULTURU/IstanbulTuru">
                    <i class="fas fa-image"></i>
                    <span>İstanbul Tur Resimleri</span>
                </a>
            </li>

            <li class="nav-item">
                <a class="nav-link" href="~/TBL_SEHIRDISITUR/Goster">
                    <i class="fas fa-images"></i>
                    <span>Şehir Dışı Tur Resimleri </span>
                </a>
                <a class="nav-link" href="~/TBL_ILADLARI/Goster">
                    <i class="fas fa-image"></i>
                    <span>Şehir Dışı Tur Ana Resim </span>
                </a>
            </li>
        </ul>
        <div id="content-wrapper">
            @RenderBody()

        </div>
    </div>
    <script src="~/Content/Admin/js/jquery.min.js"></script>
    <script src="~/Content/Admin/js/bootstrap.bundle.min.js"></script>
    <script src="~/Content/Admin/js/jquery.easing.min.js"></script>
    <script src="~/Content/Admin/js/dataTables.bootstrap4.js"></script>
    <script src="~/Content/Admin/js/sb-admin.min.js"></script>
</body>
</html>

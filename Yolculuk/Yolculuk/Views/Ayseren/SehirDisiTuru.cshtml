@model Yolculuk.Models.ViewModel
@{
    ViewBag.Title = "SehirDisiTuru";
    Layout = "~/Views/Template/Ayseren_Hizmet_Template.cshtml";
}
<div class="single-blog-img">
        <img src="~/Content/img/hizmetler/HizmetlerAnasayfa/mercedes1.jpg" style="width:100%;height:350px" alt="">
</div>
<div class="blog-text">
    <h4 style="margin-top:1%">
       @Yolculuk.Resources.HizmetlerText.sehir_disi_tur_hizmeti
    </h4>
    <p>
        <b>
            @Yolculuk.Resources.HizmetlerText.sehir_disi_tur_hizmeti_bilgi
        </b>
    </p>
</div>
@foreach (var item in Model.TBL_ILADLARI)
{
<div class="col-md-4 col-sm-4 col-xs-12 photo">
    <div class="single-awesome-project">
        <div class="awesome-img">
            <img src="~/Content/img/hizmetler/SehirDisiTur/Anaresimler/@item.slider_anaresim" alt="" />
            <h4 style="text-align:center">@item.il_adi</h4>
            <div class="add-actions text-center">
                <div class="project-dec">
                    @foreach (var item1 in Model.TBL_SEHIRDISITUR)
                    {
                        if (item.il_id == item1.il_id)
                        {
                            <a class="venobox" data-gall="@item1.il_id" href="~/Content/img/hizmetler/SehirDisiTur/@item1.slider_resim">
                            </a>
                        }
                    }
                        </div>
            </div>
        </div>
    </div>
</div>
}



